# GitHub Token for CI/CD (replace with your token)
GITHUB_TOKEN=your_github_token_here

# Repository Configuration
GITHUB_REPO_URL=https://github.com/keevingfu/eufygeo2.git
GITHUB_OWNER=keevingfu
GITHUB_REPO_NAME=eufygeo2

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=eufy_geo
DB_USER=postgres
DB_PASSWORD=postgres

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# API Configuration
PORT=5001
NODE_ENV=development
API_PREFIX=/api/v1

# Security
JWT_SECRET=your-secret-key-here-change-in-production
JWT_EXPIRES_IN=7d
BCRYPT_ROUNDS=10
CORS_ORIGIN=http://localhost:3000

# External APIs
GOOGLE_API_KEY=
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
YOUTUBE_API_KEY=
GA4_PROPERTY_ID=
GSC_SITE_URL=
SEMRUSH_API_KEY=
AHREFS_API_KEY=
REDDIT_CLIENT_ID=
REDDIT_CLIENT_SECRET=
REDDIT_USER_AGENT=

# OpenAI Integration
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4-turbo-preview

# Frontend
REACT_APP_API_URL=http://localhost:5001/api/v1
REACT_APP_WS_URL=ws://localhost:5001

# Monitoring
SENTRY_DSN=
NEW_RELIC_LICENSE_KEY=

# Email (for reports)
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASS=
REPORT_FROM_EMAIL=reports@eufy-geo.com

# File Storage
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=10485760